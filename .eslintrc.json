{
    "env": {
      "browser": true,
      "node": true,
      "es2021": true,
      "mocha": true
    },
    "overrides": [
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-unused-vars": "error",
      "no-console": "error"
    }
  }